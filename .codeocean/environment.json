{
	"version": 1,
	"base_image": "codeocean/mambaforge3:23.1.0-4-python3.10.12-ubuntu22.04",
	"post_install": true,
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true,
		"mount_secrets": true
	},
	"installers": {
		"apt": {
			"packages": [
				{
					"name": "build-essential",
					"version": "12.9ubuntu3"
				}
			],
			"install_software_properties_common": true
		},
		"pip3": {
			"packages": [
				{
					"name": "matplotlib",
					"version": "3.10.5"
				},
				{
					"name": "polars[plot,pandas]",
					"version": "1.32.3"
				},
				{
					"name": "scikit-learn",
					"version": "1.7.1"
				},
				{
					"name": "uv",
					"version": "0.8.13"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}